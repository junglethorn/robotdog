    <axis xyz="0 1 0"/>
  </joint>
  
  <joint name="body_to_tail" type="revolute">
    <parent link="base_link"/>
    <child link="tail"/>
    <axis xyz="1 0 0"/>
  	<origin rpy="0 0 0" xyz="0.2 0 0"/>
  	<limit lower="-1.5708" upper="1.5708" effort="100" velocity="0.1"/>
  </joint>
  


